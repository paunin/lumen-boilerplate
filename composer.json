{
    "name": "PlaceItUp::Core",
    "description": "PlaceItUp Core Api project",
    "authors": [
        {
            "name": "Dmitry Paunin",
            "email": "d.m.paunin@gmail.com",
            "role": "*"
        }
    ],
    "license": "proprietary",
    "require": {
        "php": "~7.1",
        "laravel/lumen-framework": "5.4.*",
        "tymon/jwt-auth": "1.0.*@dev",
        "irazasyed/larasupport": "~1.0",
        "predis/predis": "^1.0",
        "illuminate/redis": "5.4.*",
        "fzaninotto/faker": "~1.6",
        "ramsey/uuid": "^3.4",
        "swiftmailer/swiftmailer": "~5.4",
        "illuminate/mail": "~5.2",
        "relaxart/monolog-docker-handler": "1.0",
        "felixkiss/uniquewith-validator": "~2.0",
        "paunin/laravel-ps-redis": "~2.0",
        "laravel/socialite": "^3.0",
        "dingo/api": "1.0.x@dev",
        "paunin/laswagger": "^1.0",
        "intouch/laravel-newrelic": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "digitronac/coverage-checker": "^1.0",
        "m6web/symfony2-coding-standard": "~3.1",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "phpunit/dbunit": "@stable",
        "sebastian/phpcpd": "@stable",
        "mockery/mockery": "0.9.*",
        "coduo/php-matcher": "^2.0",
        "micheh/teamcity-clover": "~0.6",
        "phpmetrics/phpmetrics": "~1.10",
        "friendsofphp/php-cs-fixer": "~2.0",
        "php-mock/php-mock-phpunit": "^1.1",
        "barryvdh/laravel-ide-helper": "^2.2"
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paunin/laravel-newrelic"
        }
    ],

    "autoload": {
        "classmap": [
            "database/"
        ],
        "psr-4": {
            "App\\": "app/",
            "Seeds\\": "database/seeds"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        },
        "classmap": [
            "database/"
        ]
    },

    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "dev"
        }
    }
}
